<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd"
   >
   
   <context:annotation-config/>
   
   <bean id="employee" class="com.learn.spring.rsi.SpringRequiredAnnotation.Employee">
      <property name="name" value="Akash Sharma"></property>
      <property name="rollNum" value="07110008"></property>
   </bean>
   
   <bean id="pancard" class="com.learn.spring.rsi.SpringRequiredAnnotation.Pancard">
      <property name="panNumber" value="FYMPS5432"></property>
   </bean>
      
</beans>